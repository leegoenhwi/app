<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colortitle"
    android:weightSum="30"
    android:id="@+id/swipe_layout2">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="30"
        android:background="#FFFFFF"
        android:gravity="center">

        <!--app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"-->




            <androidx.cardview.widget.CardView
                android:layout_width="270dp"
                android:layout_height="100dp"
                android:layout_marginBottom="50dp"
                tools:ignore="DisableBaselineAlignment,MissingConstraints,UselessLeaf"
                app:cardElevation="2dp"
                app:cardUseCompatPadding="true"
                app:cardCornerRadius="8dp">



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"

                    >

                    <TextView
                        android:id="@+id/osan"
                        android:layout_width="110dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="@string/osan_stay"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.34"
                        app:layout_constraintStart_toEndOf="@+id/textView2"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.3"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/osan_num"
                        android:layout_width="30dp"
                        android:layout_height="25dp"
                        android:layout_marginStart="80dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="right"
                        android:text="@string/zero"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/textView"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/osan"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginEnd="120dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="left"
                        android:text="@string/number"
                        android:textColor="#696969"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/osan"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="43dp"
                        android:background="#323232"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginStart="31dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginEnd="31dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="인터넷"
                        android:textColor="#323232"
                        android:textSize="15sp"
                        app:layout_constraintEnd_toStartOf="@+id/osan_num"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView2"
                        tools:ignore="HardcodedText" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>





            <androidx.cardview.widget.CardView
                android:layout_width="270dp"
                android:layout_height="100dp"
                tools:ignore="DisableBaselineAlignment,MissingConstraints,UselessLeaf"
                app:cardElevation="2dp"
                app:cardUseCompatPadding="true"
                app:cardCornerRadius="8dp"
                android:layout_marginBottom="50dp">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"

                    >

                    <TextView
                        android:id="@+id/cheonan"
                        android:layout_width="110dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="@string/cheonan_stay"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.34"
                        app:layout_constraintStart_toEndOf="@+id/textView5"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.3"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/cheonan_num"
                        android:layout_width="30dp"
                        android:layout_height="25dp"
                        android:layout_marginStart="80dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="right"
                        android:text="@string/zero"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/textView4"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cheonan"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginEnd="120dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="left"
                        android:text="@string/number"
                        android:textColor="#696969"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cheonan"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="43dp"
                        android:background="#323232"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginStart="31dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginEnd="31dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="연결"
                        android:textColor="#323232"
                        android:textSize="15sp"
                        app:layout_constraintEnd_toStartOf="@+id/cheonan_num"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView5"
                        tools:ignore="HardcodedText" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>





            <androidx.cardview.widget.CardView
                android:layout_width="270dp"
                android:layout_height="100dp"
                tools:ignore="DisableBaselineAlignment,MissingConstraints,UselessLeaf"
                app:cardElevation="2dp"
                app:cardUseCompatPadding="true"
                app:cardCornerRadius="8dp">



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:id="@+id/cheonan_t"
                        android:layout_width="150dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="@string/cheonan_t_stay"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.48"
                        app:layout_constraintStart_toEndOf="@+id/textView8"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.3"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/cheonan_t_num"
                        android:layout_width="30dp"
                        android:layout_height="25dp"
                        android:layout_marginStart="80dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="right"
                        android:text="@string/zero"
                        android:textColor="#3C3C3C"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/textView7"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cheonan_t"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginEnd="120dp"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="left"
                        android:text="@string/number"
                        android:textColor="#696969"
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/cheonan_t"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="43dp"
                        android:background="#323232"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_marginStart="31dp"
                        android:layout_marginTop="7dp"
                        android:layout_marginEnd="31dp"
                        android:fontFamily="@font/spoqa_han_sans_regular"
                        android:gravity="center"
                        android:text="확인"
                        android:textColor="#323232"
                        android:textSize="15sp"
                        app:layout_constraintEnd_toStartOf="@+id/cheonan_t_num"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView8"
                        tools:ignore="HardcodedText" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>



            </LinearLayout>


</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

